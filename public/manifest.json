{
    "manifest_version": 2,
    "name": "web-monitor",
    "version": "0.1.0",
    "description": "web monitor",
    "icons":
    {
        "16": "img/icon.jpg",
        "48": "img/icon.jpg",
        "128": "img/icon.jpg"
    },
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "permissions": [
        "tabs",
        "notifications",
        "webRequest",
        "storage",
        "http://*/*",
        "https://*/*"
    ],
    "web_accessible_resources": [
        "js/inject.js"
    ],
    "options_ui": {
        "chrome_style": true,
        "page": "options.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}